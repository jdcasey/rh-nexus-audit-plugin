<?xml version="1.0"?>
<security>
  <version>2.0.3</version>
  <roles>
    <role>
      <id>audit-user</id>
      <name>Audit User</name>
      <description>Allow access to audit logs</description>
      <sessionTimeout>5</sessionTimeout>
      <privileges>
        <privilege>audit-access</privilege>
      </privileges>
    </role>
  </roles>
  <privileges>
    <privilege>
      <id>audit-access</id>
      <type>method</type>
      <name>Audit Logs: access</name>
      <description>Access to audit logs for artifacts.</description>
      <properties>
        <property>
          <key>method</key>
          <value>read</value>
        </property>
        <property>
          <key>permission</key>
          <value>nexus:audit-access</value>
        </property>
      </properties>
    </privilege>
  </privileges>
</security>