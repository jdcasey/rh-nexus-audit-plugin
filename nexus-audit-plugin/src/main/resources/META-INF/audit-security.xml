<?xml version="1.0"?>
<security>
  <version>2.0.3</version>
  <roles>
    <role>
      <id>rh-audit-user</id>
      <name>RH Audit User</name>
      <description>Allow access to audit information</description>
      <sessionTimeout>5</sessionTimeout>
      <privileges>
        <privilege>rh-audit-log-access</privilege>
      </privileges>
    </role>
  </roles>
  <privileges>
    <privilege>
      <id>rh-audit-log-access</id>
      <type>method</type>
      <name>RH Audit Info Access</name>
      <description>Access to view audit information about a path.</description>
      <properties>
        <property>
          <key>method</key>
          <value>read</value>
        </property>
        <property>
          <key>permission</key>
          <value>nexus:rh-audit-log</value>
        </property>
      </properties>
    </privilege>
  </privileges>
</security>